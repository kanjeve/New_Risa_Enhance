{
    "$schema": "vscode://schemas/color-theme",
    "name": "Asir Custom Theme", 
    "type": "light", 

    "tokenColors": [
        {
            "scope": "text",
            "settings": {
                "foreground": "#1E1E1E" 
            }
        },
        {
            "scope": [
                "comment",
                "comment.line.number-sign.rr",
                "comment.block.rr",
                "comment.block.documentation.rr"
            ],
            "settings": {
                "foreground": "#008000" 
            }
        },
        {
            "scope": "keyword.control.rr",
            "settings": {
                "foreground": "#0000FF" 
            }
        },
        {
            "scope": "support.function.builtin.rr",
            "settings": {
                "foreground": "#6A0DAD" 
            }
        },
        {
            "scope": "string.quoted.double.rr",
            "settings": {
                "foreground": "#A31515" 
            }
        },
        {
            "scope": "constant.character.escape.rr",
            "settings": {
                "foreground": "#8B0000" 
            }
        },
        {
            "scope": "constant.numeric.rr",
            "settings": {
                "foreground": "#098677" 
            }
        },
        {
            "scope": "keyword.operator.rr",
            "settings": {
                "foreground": "#0070C0" 
            }
        },
        {
            "scope": "constant.language.rr",
            "settings": {
                "foreground": "#B05F00" 
            }
        },
        {
            "scope": "storage.type.rr",
            "settings": {
                "foreground": "#007F7F" 
            }
        },
        {
            "scope": "punctuation.separator.delimiter.rr",
            "settings": {
                "foreground": "#707070" 
            }
        },
        {
            "scope": [
                "punctuation.bracket.square.rr",
                "punctuation.bracket.curly.rr",
                "punctuation.bracket.parenthesis.rr"
            ],
            "settings": {
                "foreground": "#707070" 
            }
        },
        {
            "scope": "entity.name.function.rr",
            "settings": {
                "foreground": "#993300" 
            }
        },
        {
            "scope": "variable.other.rr",
            "settings": {
                "foreground": "#000000" 
            }
        },
        {
            "scope": "entity.name.module.rr",
            "settings": {
                "foreground": "#0055AA" 
            }
        },
        {
            "scope": "entity.name.struct.rr",
            "settings": {
                "foreground": "#AA5500" 
            }
        }
    ]
}